{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "node",
        "target": "ESNext",
        "noEmitOnError": false,
        "sourceMap": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "downlevelIteration": true,
        "noFallthroughCasesInSwitch": true
    }
}