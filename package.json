{
  "name": "@tix320/noir",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "core": "npm start -w packages/core",
    "server": "npm start -w packages/server",
    "web-client": "npm start -w packages/web-client",
    "start": "concurrently --kill-others \"npm run core\" \"npm run server\" \"npm run web-client\""
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "concurrently": "^7.1.0"
  }
}
